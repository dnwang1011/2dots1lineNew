{
  "name": "2dots1line",
  "version": "1.0.0",
  "description": "Hybrid AI System + Unified Admin Panel",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "start:all": "./scripts/startup/start.sh",
    "dev": "nodemon src/index.js",
    "worker:memory": "node src/workers/memoryProcessor.worker.js",
    "worker:episode": "node src/workers/episodeAgent.worker.js",
    "lint": "eslint --config config/lint/.eslintrc.js .",
    "format": "prettier --config config/lint/.prettierrc.js --write .",
    "init-db": "node scripts/init-db.js",
    "db:seed": "npx prisma db seed",
    "db:migrate": "npx prisma migrate dev",
    "build": "npx prisma generate",
    "test": "jest --config config/jest.config.js",
    "test:unit": "jest --config config/jest.config.js --testPathPattern=tests/unit",
    "test:integration": "jest --config config/jest.config.js --testPathPattern=tests/integration",
    "test:e2e": "jest --config config/jest.config.js --testPathPattern=tests/e2e",
    "test:setup": "node tests/helpers/setup-test-db.js",
    "test:db:migrate": "npx dotenv -e tests/env/.env.test -- npx prisma migrate deploy",
    "test:weaviate": "node tests/helpers/scripts/test-weaviate-schema.js",
    "test:memory": "node tests/helpers/scripts/test-memory-retrieval.js",
    "test:episodes": "node tests/helpers/scripts/test-episode-retrieval.js",
    "test:knowledge": "node tests/helpers/scripts/test-knowledge-node.js",
    "test:job": "node tests/helpers/scripts/test-job.js",
    "test:redis": "node tests/helpers/scripts/test-redis-config.js",
    "test:cleanup": "./tests/helpers/cleanup-test-files.sh",
    "test:migrate-tools": "./tests/helpers/migrate-diagnostic-tools.sh",
    "memory:monitor": "node scripts/monitoring/monitor-memory-system.js",
    "memory:check-episodes": "node tests/helpers/scripts/diagnostics/check-weaviate-episodes.js",
    "memory:check-schema": "node tests/helpers/scripts/diagnostics/check-weaviate-schema.js",
    "memory:check-retrieval": "node tests/helpers/scripts/diagnostics/check-memory-retrieval.js",
    "memory:check-chunks": "node tests/helpers/scripts/diagnostics/check-chunks.js",
    "memory:fix-episodes": "node scripts/fixes/fix-episode-embedding.js",
    "memory:diagnose-retrieval": "node tests/helpers/scripts/diagnostics/check-memory-retrieval.js",
    "backup": "node scripts/monitoring/backup-system.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.0",
    "@prisma/client": "^5.10.0",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.51.1",
    "compute-cosine-similarity": "^1.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "density-clustering": "^1.3.0",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "form-data": "^4.0.2",
    "fs-extra": "^11.3.0",
    "gpt-tokenizer": "^2.9.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.9.0",
    "multer": "^1.4.5-lts.2",
    "neo4j-driver": "^5.28.1",
    "node-fetch": "^2.7.0",
    "node-schedule": "^2.1.1",
    "pdf-parse": "^1.1.1",
    "uuid": "^9.0.1",
    "weaviate-ts-client": "^2.2.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.5.3",
    "prisma": "^5.10.0",
    "supertest": "^7.1.0"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "keywords": []
}
